package idcard;

public class IDcard01 {

    public static void main(String[] args) {
        String in=System.in.toString();
        if (check(in)) {
            System.out.printf("%s-%s-%s\n",in.substring(6, 10),in.substring(10, 12),in.substring(12, 16));
            
        }else {
            System.out.println("0000-00-00");
        }

    }
    public static boolean check(String x) {
        if (x.length()!=18) {
            return false;
        }else {
            return true;
        }
    }

}
