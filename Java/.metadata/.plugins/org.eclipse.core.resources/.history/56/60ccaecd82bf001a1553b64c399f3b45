package idcard;
import java.time.*;
import java.util.Scanner;

public class IDcard01 {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String in=sc.next();
        if (check(in)) {
            System.out.printf("%s-%s-%s\n",in.substring(6, 10),in.substring(10, 12),in.substring(12, 16));
        }else {
            System.out.println("0000-00-00");
        }

    }
